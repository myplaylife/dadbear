<configuration>
	<client_master_protocol>
		<item>
			<name>protocol</name>
			<value>me.littlepanda.dadbear.core.rpc.ClientMasterProtocol</value>
			<type>String</type>
			<description>服务协议</description>
		</item>
		<item>
			<name>implementation</name>
			<value>me.littlepanda.dadbear.master.DefaultClientMasterProtocol</value>
			<type>String</type>
			<description>服务实现</description>
		</item>
		<item>
			<name>port</name>
			<value>56112</value>
			<type>Integer</type>
			<description>服务端口号</description>
		</item>
		<item>
			<name>type</name>
			<value>remote_avdl</value>
			<type>String</type>
			<description>远程服务类型</description>
		</item>
		<item>
			<name>description</name>
			<value>客户端与服务器你之间的通信协议</value>
			<type>String</type>
			<description>描述client-master-protocol</description>
		</item>
	</client_master_protocol>
	<slave_master_protocol>
		<item>
			<name>protocol</name>
			<value>me.littlepanda.dadbear.core.rpc.SlaveMasterProtocol</value>
			<type>String</type>
			<description>服务协议</description>
		</item>
		<item>
			<name>implementation</name>
			<value>me.littlepanda.dadbear.master.DefaultSlaveMasterProtocol</value>
			<type>String</type>
			<description>服务实现</description>
		</item>
		<item>
			<name>port</name>
			<value>56113</value>
			<type>String</type>
			<description>服务端口号</description>
		</item>
		<item>
			<name>type</name>
			<value>remote_avdl</value>
			<type>String</type>
			<description>远程服务类型</description>
		</item>
		<item>
			<name>description</name>
			<value>slave节点与master节点之间的协议，就是心跳协议</value>
			<type>String</type>
			<description>描述slave-master-protocol</description>
		</item>
	</slave_master_protocol>
